	graph [layout=dot ranksep=.3]
	node [fontname=Courier fontsize=12 height=.1 shape=circle]
	edge [arrowsize=.5]
	1 -> 2 [label=a]
	1 -> 3 [label=b]
	2 -> 2 [label=a]
	2 -> 4 [label=b]
	3 -> 2 [label=a]
	3 -> 3 [label=b]
	4 -> 2 [label=a]
	4 -> 5 [label=b]
	5 -> 2 [label=a]
	5 -> 3 [label=b]
	5 [shape=doublecircle]
}
